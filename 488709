#include<stdio.h>

int main()
{
    int E[3][3];
    int *p=E[3][3];
    int i=0,j=0;
    printf("Input elements in the matrix\n");
    for(i=0; i<3; ++i)
    {
        for(j=0; j<3;++j)
        {
            printf("Element - [%d],[%d] :",i,j);
            scanf("%d",&E[i][j]);
        }
    }
    printf("\nThe matrix is :\n");
    for(i=0; i<3; ++i)
    {
        for(j=0; j<3;++j)
        {
            printf("%d\t",E[i][j]);
        }
        printf("\n");
    }
    return 0;
}